{
    "model": {
        "type": "mlp",
        "layers": [
            2,
            50,
            50,
            50,
            50,
            1
        ],
        "activation_function": "Tanh",
        "learning_rate": 0.001
    },
    "training": {
        "epochs": 1,
        "batch_size": 32,
        "epochs_no_improve": 20,
        "min_loss_improvement": "1e-5",
        "seed": 42
    },
    "physics": {
        "nu": 0.01,
        "boundary_type": "dirichlet",
        "physics_type": "burgers"
    },
    "logging": {
        "level": "INFO",
        "output_folder": "output"
    }
}